<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>
  <ul>
    <% messages.forEach(message => { %>
      <li>
        <strong><%= message.user %></strong> (<%= message.added.toLocaleString() %>): <%= message.text %>
      </li>
    <% }); %>
  </ul>
  <a href="/new">Add a new message</a>
</body>
</html>

